module "jenkins" {
  source                      = "tf_aws_jenkins"
  agent_security_path         = "${module.s3.agent_security_path}"
  associate_public_ip_address = "${var.associate_public_ip_address}"
  cidr                        = "${var.cidr}"
  config_path                 = "${module.s3.config_path}"
  create_user_path            = "${module.s3.create_user_path}"
  creds_path                  = "${module.s3.creds_path}"
  csrf_path                   = "${module.s3.csrf_path}"
  default_path                = "${module.s3.default_path}"
  environment_type            = "${var.environment_type}"
  git_tool_path               = "${module.s3.git_tool_path}"
  instance_type               = "${lookup(var.instance_size, "jenkins")}"
  jenkins_email               = "${var.jenkins_email}"
  jenkins_password            = "${var.jenkins_password}"
  jenkins_username            = "${var.jenkins_username}"
  key_name                    = "${var.key_name}"
  maven_path                  = "${module.s3.maven_path}"
  plugins_path                = "${module.s3.plugins_path}"
  public_ip                   = "${var.public_ip}"
  public_subnet               = "${var.public_subnet}"
  region                      = "${var.region}"
  s3_bucket                   = "${module.s3.devops_bucket}"
  source_dest_check           = "${var.source_dest_check}"
  stack_name                  = "${var.stack_name}"
  tomcat_ip                   = "${module.tomcat.tomcat_ip}"
  tomcat_password             = "${var.tomcat_password}"
  tomcat_username             = "${var.tomcat_username}"
  vpc_id                      = "${var.vpc_id}"
}
